; Умножение через сложение
<input> 10 25

; загрузка данных 
read 1 ; множитль 1
read 2 ; множитель 2

load 1
sub 2

jgtz begin

; invert input
load 2
store 3
load 1
store 2
load 3
store 1

begin:
load =0
store 3 ; result

loop:
  load 3
  add 1
  store 3
  load 2
  sub =1
  store 2
  jgtz loop
write 3
Halt

